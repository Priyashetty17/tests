    stage('Execution') {
   withCredentials([string(credentialsId: 'NEUVECTOR_PASSWORD', variable: 'NEUVECTOR_PASSWORD'), string(credentialsId: 'NEUVECTOR_K8S_SERVER_13', variable: 'NEUVECTOR_K8S_SERVER_13')]) {
    sh '''
      ssh -o "StrictHostKeyChecking no" -i /home/neuvector/.ssh/id_rsa neuvector@$NEUVECTOR_K8S_SERVER_13 "/mnt/nfs/share/QA/automation/k8s_script.sh $_JOB_TYPE_ $_BUILD_TAG_ $_DIR_"
    '''
  }
 }
}
